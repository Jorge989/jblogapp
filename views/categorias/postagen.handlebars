


   <h1>{{categorias.nome}}</h1>
     {{#each postagen}}
     <div class="card mt-4">
    <div class="card-body">
        <h3>{{titulo}}</h3>
        <p>{{descricao}}</p>
       <a href="/postagen/{{slug}}"><button class="btn btn-success">Leia mais</button></a>
        <hr>
        <small>Data de publicação: {{data}}</small>
       
       </div>
   </div>
     {{else}}
    <h5>Nenhuma Postagem</h5>
      {{/each}}